                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT DISTINCT 
    a.[City] AS [City], 
    sp.[StateProvinceCode] AS [StateProvinceCode], 
    sp.[Name] AS [StateProvinceName], 
    cr.[CountryRegionCode] AS [CountryRegionCode], 
    cr.[Name] AS [EnglishCountryRegionName], 
    crfn.[SpanishCountryRegionName] AS [SpanishCountryRegionName], 
    crfn.[FrenchCountryRegionName] AS [FrenchCountryRegionName], 
    a.[PostalCode] AS [PostalCode] ,
    sp.[TerritoryID] AS SalesTerritoryAlternateKey
FROM [Person].[Address] AS a 
    JOIN [Person].[StateProvince] AS sp     ON a.[StateProvinceID] = sp.[StateProvinceID] 
    JOIN [Person].[CountryRegion] AS cr     ON sp.[CountryRegionCode] = cr.[CountryRegionCode] 
    JOIN person.[Address] AS ca     ON a.[AddressID] = ca.[AddressID] 
	JOIN Person.BusinessEntityAddress pe on pe.AddressID=a.AddressID 
    --JOIN [Sales].[Customer] AS c     ON pe.BusinessEntityID = c.[CustomerID] 
    --JOIN [Sales].[SalesTerritory] AS st     ON c.[TerritoryID] = st.[TerritoryID] 
    LEFT JOIN [AdventureWorks2012_STG].[dbo].[tempCountryRegion-ForeignNames] AS crfn 
    ON sp.[CountryRegionCode] = crfn.[CountryRegionCode] 
WHERE ( sp.[TerritoryID] IS NOT NULL)
ORDER BY cr.[CountryRegionCode], sp.[StateProvinceCode], a.[City];</property>
